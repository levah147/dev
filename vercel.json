{
    "version": 2,
    "build": [{
        "src": "manage.py/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "Python 3.12.7" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "manage.py/wsgi.py"
        }
    ]    
}
